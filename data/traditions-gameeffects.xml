<?xml version="1.0" encoding="utf-8"?>
<GameEffects xmlns="GameEffects">

    <!-- TRADITION_HITTITE_RITUAL_PITS -->
	<Modifier id="MOD_JEC_RITUAL_PITS_CULTURE" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_GRANT_WAREHOUSE_YIELD">
		<Argument name="WarehouseYieldChange">HittiteRitualPitsClayPitsCulture, HittiteRitualPitsMineCulture, HittiteRitualPitsMineResouceCulture, HittiteRitualPitsQuarryCulture</Argument>
		<Argument name="Tooltip">LOC_TRADITION_HITTITE_RITUAL_PITS_NAME</Argument>
		<String context="Description">LOC_MOD_JEC_RITUAL_PITS_CULTURE_DESCRIPTION</String>
	</Modifier>
	<Modifier id="MOD_JEC_RITUAL_PITS_PRODUCTION" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_GRANT_WAREHOUSE_YIELD">
		<Argument name="WarehouseYieldChange">HittiteRitualPitsClayPitsProudction, HittiteRitualPitsMineProudction, HittiteRitualPitsMineResouceProudction, HittiteRitualPitsQuarryProudction</Argument>
		<Argument name="Tooltip">LOC_TRADITION_HITTITE_RITUAL_PITS_NAME</Argument>
		<String context="Description">LOC_MOD_JEC_RITUAL_PITS_PRODUCTION_DESCRIPTION</String>
	</Modifier>

    <!-- TRADITION_HITTITE_GIFTS_OF_THE_STORM_GOD -->
	<Modifier id="MOD_JEC_GIFTS_OF_THE_STORM_GOD_HAPPINESS_UNASSIGNED_RESOURCE" collection="COLLECTION_OWNER" effect="EFFECT_PLAYER_ADJUST_YIELD_PER_RESOURCE">
		<Argument name="YieldType">YIELD_HAPPINESS</Argument>
		<Argument name="Amount">2</Argument>
		<Argument name="Unassigned">true</Argument>
		<String context="Description">LOC_MOD_JEC_GIFTS_OF_THE_STORM_GOD_HAPPINESS_UNASSIGNED_RESOURCE_DESCRIPTION</String>
	</Modifier>
	
    <!-- TRADITION_HITTITE_OATH_OF_LOYALTY -->
	<Modifier id="MOD_JEC_OATH_OF_LOYALTY_PACKED_UNITS" collection="COLLECTION_COMMANDER_PACKED_UNITS" effect="EFFECT_UNIT_ADJUST_PLAYER_YIELD">
		<Argument name="YieldType">YIELD_HAPPINESS</Argument>
		<Argument name="Amount">1</Argument>
		<String context="Description">LOC_MOD_JEC_OATH_OF_LOYALTY_PACKED_UNITS_DESCRIPTION</String>
	</Modifier>

	<!-- TRADITION_HITTITE_PANKUS -->
	<Modifier id="MOD_JEC_PANKUS_TOWNS" collection="COLLECTION_OWNER" effect="EFFECT_PLAYER_ADJUST_YIELD_PER_RESOURCE">
		<SubjectRequirements>
			<Requirement type="REQUIREMENT_CITY_IS_TOWN"/>
		</SubjectRequirements>
		<Argument name="YieldType">YIELD_HAPPINESS</Argument>
		<Argument name="Amount">3</Argument>
		<Argument name="Imported">false</Argument>
		<Argument name="Tooltip">LOC_TRADITION_HITTITE_PANKUS_NAME</Argument>
		<String context="Description">LOC_MOD_JEC_PANKUS_TOWNS_DESCRIPTION</String>
	</Modifier>
	<Modifier id="MOD_JEC_PANKUS_CITIES" collection="COLLECTION_OWNER" effect="EFFECT_PLAYER_ADJUST_YIELD_PER_RESOURCE">
		<SubjectRequirements>
			<Requirement type="REQUIREMENT_CITY_IS_CITY"/>
		</SubjectRequirements>
		<Argument name="YieldType">YIELD_HAPPINESS</Argument>
		<Argument name="Amount">1</Argument>
		<Argument name="Imported">false</Argument>
		<Argument name="Tooltip">LOC_TRADITION_HITTITE_PANKUS_NAME</Argument>
		<String context="Description">LOC_MOD_JEC_PANKUS_CITIES_DESCRIPTION</String>
	</Modifier>

</GameEffects>